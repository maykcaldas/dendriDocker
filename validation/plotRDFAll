unset multiplot
set terminal pngcairo size 1800,1500 enhanced font 'Verdana-Bold,10'
set output "PAMAMRDF_nonorm.png"

set key right font @keyFont 

set border front lc 0 lt 1 lw 3
set grid back linecolor 0 lt 0 lw 2

set multiplot layout 3,3 columnsfirst

set xlabel "r[nm]" font @labelFont
set xtics font @ticsFont
set xrange [0:1]

set ytics font @ticsFont
set yrange [0:450]
set ylabel "g(r): Monomer-CM\n" font @labelFont

#-------------------------------//------------------------------#

set key left font @keyFont at 0.4,1700
set title "Low pH" font @titleFont

plot "../rdf_G0_Acid.xvg" using 1:2 title "G0" with lines ls 1, \
     "../rdf_G1_Acid.xvg" using 1:2 title "G1" with lines ls 2, \
     "../rdf_G2_Acid.xvg" using 1:2 title "G2" with lines ls 3, \
     "../rdf_G3_Acid.xvg" using 1:2 title "G3" with lines ls 4, \
     "../rdf_G4_Acid.xvg" using 1:2 title "G4" with lines ls 5, \
     "../rdf_G5_Acid.xvg" using 1:2 title "G5" with lines ls 6, \
 
#-------------------------------//------------------------------#

set title "Neutral pH" font @titleFont

plot "../rdf_G0_Neutral.xvg" using 1:2 title "G0" with lines ls 1, \
     "../rdf_G1_Neutral.xvg" using 1:2 title "G1" with lines ls 2, \
     "../rdf_G2_Neutral.xvg" using 1:2 title "G2" with lines ls 3, \
     "../rdf_G3_Neutral.xvg" using 1:2 title "G3" with lines ls 4, \
     "../rdf_G4_Neutral.xvg" using 1:2 title "G4" with lines ls 5, \
     "../rdf_G5_Neutral.xvg" using 1:2 title "G5" with lines ls 6, \
 
