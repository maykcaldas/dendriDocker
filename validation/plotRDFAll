set terminal pngcairo size 1800,1500 enhanced font 'Verdana-Bold,10'
#set output "PAMAMTerRDF.png"
set output "PAMAMRDF.png"

#Setting fonts#

titleFont="'Verdana-Bold,20'"
labelFont="'Verdana-Bold,18'"
ticsFont="'Verdana-Bold,16'"
keyFont="'Verdana-Bold,14'"

#Setting the pallete#

set style line 8 lc rgb "black" lt 1 lw 3       # error bar / Liu2009
set style line 1 lc rgb "red" lt 1 lw 3         # 2016H66
set style line 2 lc rgb "blue" lt 1 lw 3        # Maingi2012
set style line 3 lc rgb "#008B8B" lt 1 lw 3     # Maiti2004
set style line 4 lc rgb "#FF8C00" lt 1 lw 3     # Maiti2005
set style line 5 lc rgb "green" lt 1 lw 3       # Opitz2006
set style line 6 lc rgb "#9400D3" lt 1 lw 3     # Lee2002
set style line 7 lc rgb "#1E90FF" lt 1 lw 3     # Caballero2013
set style line 9 lc rgb "#8B0000" lt 1 lw 3     # Barraza2018
set style line 10 lc rgb "#006b00" lt 1 lw 3    # Gromos-Kanchi2018
set style line 11 lc rgb "#ff32fd" lt 1 lw 3    # CHARMM-Kanchi2018
set style line 12 lc rgb "#ff32fd" lt 1 lw 3    # Porcar2008
set style line 13 lc rgb "#ff32fd" lt 1 lw 3    # Prosa1997
set style line 14 lc rgb "#ff32fd" lt 1 lw 3    # Rathgeber2002
set style line 15 lc rgb "#ff32fd" lt 1 lw 3    # Freire2016
set style line 16 lc rgb "#ff32fd" lt 1 lw 3    # Tanis2009

