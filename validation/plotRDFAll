#unset multiplot
#set terminal pngcairo size 600,1500 enhanced font 'Verdana-Bold,10'
#set output "PAMAMMonomerRDF.png"

set key right font @keyFont

set border front lc 0 lt 1 lw 3
set grid back linecolor 0 lt 0 lw 2

set multiplot layout 3,3 columnsfirst

set xlabel "r[nm]" font @labelFont
set xtics font @ticsFont
set xrange [0:5]

set ytics font @ticsFont
set yrange [0:1800]
set ylabel "(4πr²Δr)g(r): Monomer-CM\n" font @labelFont

#-------------------------------//------------------------------#

set title "Low pH" font @titleFont

plot "../rdf_G0_Acid_nonorm.xvg" using 1:2 title "G0" with lines ls 1, \
     "../rdf_G1_Acid_nonorm.xvg" using 1:2 title "G1" with lines ls 2, \
     "../rdf_G2_Acid_nonorm.xvg" using 1:2 title "G2" with lines ls 3, \
     "../rdf_G3_Acid_nonorm.xvg" using 1:2 title "G3" with lines ls 4, \
     "../rdf_G4_Acid_nonorm.xvg" using 1:2 title "G4" with lines ls 5, \
     "../rdf_G5_Acid_nonorm.xvg" using 1:2 title "G5" with lines ls 6, \
 
#-------------------------------//------------------------------#

set title "Neutral pH" font @titleFont

plot "../rdf_G0_Neutral_nonorm.xvg" using 1:2 title "G0" with lines ls 1, \
     "../rdf_G1_Neutral_nonorm.xvg" using 1:2 title "G1" with lines ls 2, \
     "../rdf_G2_Neutral_nonorm.xvg" using 1:2 title "G2" with lines ls 3, \
     "../rdf_G3_Neutral_nonorm.xvg" using 1:2 title "G3" with lines ls 4, \
     "../rdf_G4_Neutral_nonorm.xvg" using 1:2 title "G4" with lines ls 5, \
     "../rdf_G5_Neutral_nonorm.xvg" using 1:2 title "G5" with lines ls 6, \
 
#-------------------------------//------------------------------#

set title "High pH" font @titleFont

plot "../rdf_G0_Basic_nonorm.xvg" using 1:2 title "G0" with lines ls 1, \
     "../rdf_G1_Basic_nonorm.xvg" using 1:2 title "G1" with lines ls 2, \
     "../rdf_G2_Basic_nonorm.xvg" using 1:2 title "G2" with lines ls 3, \
     "../rdf_G3_Basic_nonorm.xvg" using 1:2 title "G3" with lines ls 4, \
     "../rdf_G4_Basic_nonorm.xvg" using 1:2 title "G4" with lines ls 5, \
