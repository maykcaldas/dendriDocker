set terminal pngcairo size 2200,600 enhanced font 'Verdana-Bold,10'
set output "PAMAMRg.png"

#Setting fonts#

titleFont="'Verdana-Bold,20'"
labelFont="'Verdana-Bold,18'"
ticsFont="'Verdana-Bold,16'"
keyFont="'Verdana-Bold,14'"

#Setting the pallete#

set style line 8 lc rgb "black"         lt 1 dt 1 pt 1 lw 3 ps 1    # error bar / Liu2009 / Wu2010
set style line 1 lc rgb "red"           lt 1 dt 1 pt 1 lw 3 ps 1   # 2016H66
set style line 2 lc rgb "blue"          lt 1 dt 1 pt 1 lw 3 ps 1    # Maingi2012
set style line 3 lc rgb "green"         lt 1 dt 1 pt 1 lw 3 ps 1   # Maiti2004 / Jain2013
set style line 4 lc rgb "dark-green"    lt 1 dt 1 pt 1 lw 3 ps 1   # Maiti2005 
set style line 5 lc rgb "dark-cyan"     lt 1 dt 1 pt 1 lw 3 ps 1    # Opitz2006 
set style line 6 lc rgb "#9a0099"       lt 1 dt 1 pt 1 lw 3 ps 1    # Lee2002   (dark-purple)
set style line 7 lc rgb "#f000f2"       lt 1 dt 1 pt 1 lw 3 ps 1    # Caballero2013 (pink)
set style line 9 lc rgb "orange"        lt 1 dt 1 pt 1 lw 3 ps 1    # Barraza2018
set style line 10 lc rgb "#006b00"      lt 1 dt 1 pt 6 lw 3 ps 2    # Gromos-Kanchi2018
set style line 11 lc rgb "red"          lt 1 dt 1 pt 6 lw 3 ps 2    # CHARMM-Kanchi2018
set style line 12 lc rgb "dark-green"   lt 1 dt 2 pt 8 lw 3 ps 2   # Porcar2008 / Topp1998
set style line 13 lc rgb "blue"         lt 1 dt 2 pt 8 lw 3 ps 2   # Prosa1997
set style line 14 lc rgb "dark-orange"  lt 1 dt 2 pt 8 lw 3 ps 2   # Rathgeber2002 / Scherrenberg1998
set style line 15 lc rgb "#646464"      lt 1 dt 1 pt 1 lw 3 ps 1    # Freire2016 (dark-gray)
set style line 16 lc rgb "dark-red"     lt 1 dt 1 pt 1 lw 3 ps 1    # Tanis2009

#####################

set key left font @keyFont 

set border front lc 0 lt 1 lw 4
set grid back linecolor 0 lt 0 lw 2

set multiplot layout 1,3

set xlabel "Generation" font @labelFont
set xtics font @ticsFont
set xrange [0:5]

set ytics font @ticsFont
set yrange [0:4]
set ylabel "R_g[nm]" font @labelFont

#-------------------------------//------------------------------#

set title "Low pH" font @titleFont

plot "../media/gyrateGAcid.txt" using 1:2:3 title "" with yerrorbars ls 8, \
     "../media/gyrateGAcid.txt" using 1:2 title "2016H66" with lines ls 1, \
     "../media/gyrateGAcidMA05.Y.txt" using 1:2 title "Dreiding-Maiti2005" with linespoints ls 4, \
     "../media/gyrateGAcidOP06.Y.txt" using 1:2 title "GAFF-Opitz2006" with linespoints ls 5, \
     "../media/gyrateGAcidLE02.Y.txt" using 1:2 title "CVFF-Lee2002" with linespoints ls 6, \
     "../media/gyrateGAcidPO08.Y.txt" using 1:2 title "SANS-Porcar2008" with linespoints ls 12, \

#-------------------------------//------------------------------#

set title "Neutral pH" font @titleFont

set key left font 'Verdana-Bold,12'

plot "../media/gyrateGNeutral.txt" using 1:2:3 title "" with yerrorbars ls 8, \
     "../media/gyrateGNeutral.txt" using 1:2 title "2016H66" with lines ls 1, \
     "../media/gyrateGNeutralMA12.Y.txt" using 1:2 title "GAFF-Maingi2012" with linespoints ls 2, \
     "../media/gyrateGNeutralMA05.Y.txt" using 1:2 title "Dreiding-Maiti2005" with linespoints ls 4, \
     "../media/gyrateGNeutralOP06.Y.txt" using 1:2 title "GAFF-Opitz2006" with linespoints ls 5, \
     "../media/gyrateGNeutralLE02.Y.txt" using 1:2 title "CVFF-Lee2002" with linespoints ls 6, \
     "../media/gyrateGNeutralCA13.4.txt" using 1:2 title "CHARMM-Caballero2013" with linespoints ls 7, \
     "../media/gyrateGNeutralBA18.Y.txt" using 1:2 title "GAFF-Barraza2018" with linespoints ls 9, \
     "../media/gyrateGNeutralPO08.Y.txt" using 1:2 title "SANS-Porcar2008" with linespoints ls 12, \
     "../media/gyrateGNeutralPR97.Y.txt" using 1:2 title "SAXS-Prosa1997" with linespoints ls 13, \
     "../media/gyrateGNeutralRA02.Y.txt" using 1:2 title "SAXS-Rathgeber2002" with linespoints ls 14, \
     "../media/gyrateGNeutralGKA18.Y.txt" using 1:2 title "CHARMM-Kanchi2018" with points ls 11, \
     "../media/gyrateGNeutralCKA18.Y.txt" using 1:2 title "GROMOS-Kanchi2018" with points ls 10, \
#-------------------------------//------------------------------#

set title "High pH" font @titleFont 

set key left font @keyFont

plot "../media/gyrateGBasic.txt" using 1:2:3 title "" with yerrorbars ls 8, \
     "../media/gyrateGBasic.txt" using 1:2 title "2016H66" with lines ls 1, \
     "../media/gyrateGBasicMA12.Y.txt" using 1:2 title "GAFF-Maingi2012" with linespoints ls 2, \
     "../media/gyrateGBasicMA04.Y.txt" using 1:2 title "Dreiding-Maiti2004" with linespoints ls 3, \
     "../media/gyrateGBasicMA05.Y.txt" using 1:2 title "Dreiding-Maiti2005" with linespoints ls 4, \
     "../media/gyrateGBasicOP06.Y.txt" using 1:2 title "GAFF-Opitz2006" with linespoints ls 5, \
     "../media/gyrateGBasicLE02.Y.txt" using 1:2 title "CVFF-Lee2002" with linespoints ls 6, \
     "../media/gyrateGBasicBA18.Y.txt" using 1:2 title "GAFF-Barraza2018" with linespoints ls 9, \
     "../media/gyrateGBasicPO08.Y.txt" using 1:2 title "SANS-Porcar2008" with linespoints ls 12, \
     

#######################################################################
unset multiplot

set terminal pngcairo size 2200,600 enhanced font 'Verdana-Bold,10'
set output "PAMAMgyrateGpH.png"

set key right font @keyFont

set border front lc 0 lt 1 lw 4
set grid back linecolor 0 lt 0 lw 2

set multiplot layout 1,3

set xlabel "pH" font @labelFont 
set xtics font @ticsFont
set xtics offset 0.5
set xrange [3:11]

set ytics font @ticsFont
set yrange [0.5:4]
set ylabel "Rg[nm]" font @labelFont

unset logscale
unset label

#set autoscale

#-------------------------------//------------------------------#

set title "Generation 3" font @titleFont

plot "../media/gyrateG3pH.txt" using 2:3:xtic(1) title "2016H66" with lines ls 1, \
     "../media/gyrateG3pHMA12.Y.txt" using 2:3:xtic(1) title "GAFF-Maingi2012" with linespoints ls 2, \
     "../media/gyrateG3pHOP06.Y.txt" using 2:3:xtic(1) title "Dreiding-Opitz2006" with linespoints ls 5, \
